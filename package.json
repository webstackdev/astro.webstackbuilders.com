  "organization": "Webstack Builders",
  "author": "Kevin Brown <kevin@webstackbuilders.com> (https://webstackbuilders.com/)",
  "domain": "webstackbuilders.com",
  "email": "sales@webstackbuilders.com",
  "lang": "en",
  "locale": "en_US",
  "keywords": [],
  "license": "AGPL-3.0",
  "repository": "https://github.com/webstackbuilders/webstack-builders-corporate-website",
  "bugs": {
    "url": "https://github.com/webstackbuilders/webstack-builders-corporate-website/issues"
  },
  "private": "true",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "type": "module",
  "browserslist": [
    "> 1%",
    "not IE 11"
  ],
  "scripts": {
    "build": "NODE_ENV=production npm run build:favicons && npx astro build",
    "build:dev": "NODE_ENV=development npm run build:favicons && npx astro build",
    "build:favicons": "npx ts-node ./scripts/build/favicon.ts",
    "check": "npx astro check",
    "clean": "npx rimraf dist && npx rimraf .astro",
    "dev": "npm run sync && npm run build:favicons && NODE_ENV=development npx astro dev",
    "format": "npm run format:json && npm run format:code && npm run format:style",
    "format:code": "npx prettier --write \"@types/**/*.{js,ts}\" \"scripts/**/*.{js,ts}\" \"src/**/*.{js,ts,tsx,astro}\" --plugin=prettier-plugin-astro",
    "format:json": "npx prettier --write '**/*.json' --cache --ignore-path .gitignore",
    "format:style": "npx stylelint --fix \"src/**/*.{css,astro}\"",
    "lint": "npm run lint:json && npm run lint:style && npm run lint:code",
    "lint:code": "npx eslint \"@types/**/*.{js,ts}\" \"scripts/**/*.{js,ts}\" \"src/**/*.{js,ts,tsx,astro}\"",
    "lint:json": "npx prettier --write '**/*.json' --cache --ignore-path .gitignore",
    "lint:style": "npx stylelint \"src/**/*.{css,astro}\"",
    "preview": "npm run build:favicons && npx astro build && astro preview",
    "sync": "npx astro sync",
    "test": "NODE_ENV=test npm run test:unit && npm run test:e2e:ready",
    "test:coverage": "NODE_ENV=test npx vitest run --coverage",
    "test:e2e": "NODE_ENV=test npx playwright test",
    "test:unit": "NODE_ENV=test npx vitest run",
    "upgrade": "npx @astrojs/upgrade",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/check": "0.9.5",
    "@astrojs/mdx": "4.3.9",
    "@astrojs/preact": "4.1.2",
    "@astrojs/rss": "4.0.13",
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/vercel": "^9.0.0",
