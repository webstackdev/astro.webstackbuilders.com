{
  "name": "webstackbuilders",
  "version": "0.0.1",
  "title": "Webstack Builders Company Website",
  "description": "This is the company website for Webstack Builders",
  "organization": "Webstack Builders",
  "author": "Kevin Brown <kevin@webstackbuilders.com> (https://webstackbuilders.com/)",
  "domain": "webstackbuilders.com",
  "email": "sales@webstackbuilders.com",
  "lang": "en",
  "locale": "en_US",
  "keywords": [],
  "license": "MIT",
  "repository": "https://github.com/webstackbuilders/webstack-builders-corporate-website",
  "bugs": {
    "url": "https://github.com/webstackbuilders/webstack-builders-corporate-website/issues"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.1.0",
    "yarn": "please-use-npm"
  },
  "type": "module",
  "packageManager": "npm@10.8.3",
  "browserslist": [
    "> 1%",
    "not IE 11"
  ],
  "scripts": {
    "build": "npm run build:sprites && npx astro build",
    "build:favicons": "npx ts-node ./scripts/build/favicon.ts",
    "build:sprites": "npx gulp build:sprites",
    "check": "npx astro check",
    "clean": "npx rimraf dist && npx rimraf .astro",
    "dev": "npm run build:sprites && npm run sync && npx astro dev",
    "lint": "npm run lint:format && npm run lint:style && npm run lint:code",
    "lint:code": "npx eslint \"src/**/*.{ts,tsx,astro}\"",
    "lint:format": "npx prettier --write . --plugin=prettier-plugin-astro",
    "lint:style": "npx stylelint \"src/**/*.{css,astro,ts,tsx}\"",
    "preview": "npx astro build && astro preview",
    "sync": "npx astro sync",
    "test": "npm run test:unit",
    "test:e2e": "npx playwright test",
    "test:unit": "npx vitest",
    "upgrade": "npx @astrojs/upgrade"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/mdx": "4.0.0-beta.2",
    "@astrojs/preact": "3.5.3",
    "@astrojs/prefetch": "0.4.1",
    "@astrojs/rss": "4.0.9",
    "@astrojs/tailwind": "5.1.2",
    "@tailwindcss/forms": "0.5.9",
    "@tailwindcss/typography": "0.5.15",
    "ansi-colors": "4.1.3",
    "astro": "5.0.0-beta.4",
    "astro-breadcrumbs": "3.2.0",
    "astro-purgecss": "4.6.1",
    "autoprefixer": "10.4.20",
    "classnames": "2.5.1",
    "convert-svg-to-png": "0.6.4",
    "cssnano": "7.0.6",
    "dotenv": "16.4.5",
    "fancy-log": "2.0.0",
    "focus-trap": "7.6.0",
    "gulp": "5.0.0",
    "gulp-header": "2.0.9",
    "gulp-rename": "2.0.0",
    "gulp-run-command": "0.0.10",
    "gulp-svg-sprite": "2.0.3",
    "libphonenumber-js": "1.11.11",
    "postcss": "8.4.47",
    "postcss-styled-syntax": "0.6.4",
    "preact": "^10.24.3",
    "purgecss": "6.0.0",
    "sass": "1.79.5",
    "sharp": "0.33.5",
    "svg-sprite": "2.0.4",
    "tailwindcss": "3.4.14",
    "title-case": "4.3.2",
    "to-ico": "1.1.5",
    "ts-node": "10.9.2",
    "workbox-build": "7.1.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "1.48.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/preact": "3.2.4",
    "@testing-library/user-event": "14.5.2",
    "@types/cross-spawn": "6.0.6",
    "@types/fancy-log": "2.0.2",
    "@types/gulp": "4.0.17",
    "@types/gulp-header": "2.0.4",
    "@types/gulp-rename": "2.0.6",
    "@types/gulp-svg-sprite": "1.2.36",
    "@types/node": "22.7.5",
    "@types/svg-sprite": "0.0.39",
    "@types/to-ico": "1.1.3",
    "@types/yargs": "17.0.33",
    "@typescript-eslint/eslint-plugin": "8.9.0",
    "@typescript-eslint/parser": "8.8.1",
    "cross-spawn": "7.0.3",
    "eslint": "9.12.0",
    "eslint-plugin-astro": "1.3.0",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "jsdom": "25.0.1",
    "postcss-html": "1.7.0",
    "prettier": "3.3.3",
    "prettier-plugin-astro": "0.14.1",
    "rimraf": "6.0.1",
    "stylelint": "16.10.0",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-declaration-block-no-ignored-properties": "2.8.0",
    "stylelint-order": "6.0.4",
    "stylelint-scss": "6.7.0",
    "temp-dir": "3.0.0",
    "tslib": "2.8.0",
    "typescript": "5.6.3",
    "vitest": "2.1.3",
    "yargs": "17.7.2"
  },
  "overrides": {
    "astro-purgecss": {
      "astro": "5.0.0-beta.4"
    },
    "react": "npm:@preact/compat@latest",
    "react-dom": "npm:@preact/compat@latest"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.24.0"
  }
}
