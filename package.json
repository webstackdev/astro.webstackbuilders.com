  "scripts": {
    "build": "npm run build:sprites && npx astro build",
    "build:favicons": "npx ts-node ./scripts/build/favicon.ts",
    "build:sprites": "npx gulp build:sprites",
    "check": "npx astro check",
    "clean": "npx rimraf dist && npx rimraf .astro",
    "dev": "npm run build:sprites && npm run sync && npx astro dev",
    "lint": "npm run lint:format && npm run lint:style && npm run lint:code",
    "lint:code": "npx eslint \"src/**/*.{ts,tsx,astro}\"",
    "lint:format": "npx prettier --write . --plugin=prettier-plugin-astro",
    "lint:style": "npx stylelint \"src/**/*.{css,astro,ts,tsx}\"",
    "preview": "npx astro build && astro preview",
    "sync": "npx astro sync",
    "test": "npm run test:unit",
    "test:e2e": "npx playwright test",
    "test:unit": "npx gulp test:unit",
    "upgrade": "npx @astrojs/upgrade"
  },
