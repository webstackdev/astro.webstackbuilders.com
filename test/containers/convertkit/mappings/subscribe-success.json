{
  "request": {
    "method": "POST",
    "urlPathPattern": "/v3/forms/.+/subscribe",
    "queryParameters": {
      "api_key": {
        "matches": ".+"
      }
    },
    "headers": {
      "content-type": {
        "contains": "application/json"
      }
    }
  },
  "response": {
    "status": 200,
    "bodyFileName": "subscribe-success-response.json",
    "headers": {
      "Content-Type": "application/json",
      "X-Mock-Service": "convertkit"
    },
    "transformers": ["response-template"]
  }
}
