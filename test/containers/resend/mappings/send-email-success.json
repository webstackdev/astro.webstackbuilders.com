{
  "request": {
    "method": "POST",
    "urlPath": "/emails",
    "headers": {
      "Authorization": {
        "matches": "Bearer .*"
      }
    }
  },
  "response": {
    "status": 202,
    "body": "{\n  \"id\": \"mock_resend_{{randomValue length=8 type='ALPHANUMERIC'}}\",\n  \"object\": \"email\",\n  \"to\": \"{{jsonPath request.body '$.to'}}\",\n  \"created_at\": \"{{now format='yyyy-MM-dd\\'T\\'HH:mm:ssXXX'}}\",\n  \"subject\": \"{{jsonPath request.body '$.subject'}}\",\n  \"status\": \"queued\"\n}",
    "headers": {
      "Content-Type": "application/json",
      "X-Mock-Service": "resend"
    },
    "transformers": ["response-template"]
  }
}
