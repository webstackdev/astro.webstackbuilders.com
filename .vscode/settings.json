{
  "files.associations": {
    "*.mdx": "markdown",
    "*.scss": "tailwindcss"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "astro",
    "typescript",
    "typescriptreact"
  ],
  "stylelint.validate": [
    "html",
    "astro"
  ],
  "cSpell.words": [
    "bezier",
    "Btns",
    "cfduid",
    "Favicons",
    "Joomla",
    "KHTML",
    "oklch",
    "shiki",
    "signup",
    "spriter",
    "squoosh",
    "Tscompile",
    "tsdoc",
    "tseslint",
    "tydata",
    "unspaced",
    "uppy",
    "virt"
  ],
  "testing.openTesting": "neverOpen",
  "testing.automaticallyOpenTestResults": "neverOpen"
}
