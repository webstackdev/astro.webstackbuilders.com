---
import BaseLayout from '@layouts/BaseLayout.astro'
import ContactPage from '@components/Pages/Contact/index.astro'

const pageTitle = 'Contact Us'
const description =
  'Connect with the Webstack Builders team to discuss your project, request a proposal, or ask questions.'
---

<BaseLayout pageTitle={pageTitle} path={"/contact"} description={description}>
  <ContactPage content={{
    header: {
      title: "Let's Build Something Amazing",
      description: "Ready to transform your digital presence? Share your project details and let's create something extraordinary together.",
    },
  }} />
</BaseLayout>
