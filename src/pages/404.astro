---
import PageLayout from '@layouts/PageLayout.astro'
import NotFound from '@components/Pages/NotFound/index.astro'

const pageDescription = "Sorry, the page you are looking for doesn't exist or has been moved."
const pageTitle = "404 - Page Not Found"
const path = '/404/'
---

<PageLayout
  pageTitle={pageTitle}
  pageDescription={pageDescription}
  icon="error"
  path={path}
  noindex={true}
>
  <NotFound
    content={{
      buttonText: "Go Back Home",
    }}
  />
</PageLayout>
